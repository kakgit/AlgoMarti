                                                    <div class="form-group">
                                                        <label for="txtUserAPIKey">API Key</label>
                                                        <input type="text" id="txtUserAPIKey" class="form-control" value="zhckjOdxFmHCTFx664ZpJ5H7Oxb3Db" placeholder="API Key" />
                                                    </div>
                                                </div>
                                                <div class="col-md-12 col-12">
                                                    <div class="form-group">
                                                        <label for="txtAPISecret">API Secret Code</label>
                                                        <input type="text" id="txtAPISecret" class="form-control" value="IqSL4kBct7CakpEDH9grdWuAgjM0zg4KgB3yGoRjj4xPZm8Qz57LWLaVWqCK" placeholder="API Secret" />
                                                    </div>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-light-secondary" data-bs-dismiss="modal">
                    <i class="bx bx-x d-block d-sm-none"></i>
                    <span class="d-none d-sm-block">CLOSE</span>
                </button>
                <button id="btnSubmit" type="button" class="btn btn-primary ms-1" onclick="fnValidateDeltaLogin();">
                    <i class="bx bx-check d-block d-sm-none"></i>
                    <span class="d-none d-sm-block">SUBMIT</span>
                </button>
            </div>
        </div>
    </div>
</div>

